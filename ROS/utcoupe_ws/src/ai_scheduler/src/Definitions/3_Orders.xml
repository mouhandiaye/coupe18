<orders>
	<!-- Actuators -->
	<order ref="wait_for_jack" duration="1">
		<message dest="feedback/display/reply_when_jack">
			<param name="display_num" type="int" optional="true"/>
			<param name="display_font" type="string" >Arial</param>
		</message>
	</order>

	<order ref="funny_action" duration="1">
		<message dest="movement/actuators/funny_action">

		</message>
	</order>

	<order ref="actuator_startup_sequence">
		<message dest="movement/actuators/actuators_startup_sequence">
			<param name="actuator_id" type="int"/>
		</message>
	</order>

	<order ref="actuator_toggle" duration="1.5" reward="3">
		<message dest="movement/actuators/actuator_toggle">
			<param name="actuator_id" type="int"/>
		</message>
	</order>

	<order ref="actuator_open" duration="1.5" reward="3">
		<message dest="movement/actuators/actuator_open">
			<param name="actuator_id" type="int"/>
		</message>
	</order>

	<order ref="actuator_close" duration="1.5" reward="3">
		<message dest="movement/actuators/actuator_close">
			<param name="actuator_id" type="int"/>
		</message>
	</order>



	<!-- Wheels -->
	<order ref="wheels_goto" duration="1">
		<message dest="wheels/goto">
			<param name="pos_x" type="float"/>
			<param name="pos_y" type="float"/>
		</message>
	</order>



	<!-- AI Specific -->
	<order ref="ai_delay" duration="1">
		<message dest="ai/scheduler/ai_delay">
			<param name="delay" type="float">0.75</param>
		</message>
	</order>

	<order ref="timer_start">
		<message dest="ai/timer/timer_start">

		</message>
	</order>

	<order ref="timer_set_duration">
		<message dest="ai/timer/timer_set">
			<param name="delay" type="float">0.75</param>
		</message>
	</order>

</orders>
